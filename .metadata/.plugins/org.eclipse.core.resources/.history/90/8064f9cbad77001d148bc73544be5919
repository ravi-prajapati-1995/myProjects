class NewThread implements Runnable {
	
	String name;
	Thread t;

	public NewThread(String name) {
		this.name = name;
		t = new Thread(this,name);
		System.out.println("New Thread: "+t);
	}
	
	@Override
	public void run() {
		
		try {
			
		} catch (InterruptedException e) {
			
		}
		
	}
	
}
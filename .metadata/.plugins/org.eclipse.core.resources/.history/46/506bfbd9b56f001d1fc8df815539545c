package com.test;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

import com.test.expamples.MyMath;

class MathTest {

	@Test
	void test() {
		MyMath math = new MyMath();
		int arr[] = {1,3,4,2};
		int result = math.sum(arr);
		int expected = 10;
		assertEquals(expected,result);
	}
	
	@Test
	void test2() {
		MyMath math = new MyMath();
		int arr[] = {};
		int result = math.sum(arr);
		int expected = 10;
		assertEquals(expected,result);
	}

}
